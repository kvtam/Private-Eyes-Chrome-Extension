{
	"manifest_version": 2,
	"name": "PrivateEyesAutoFill",
	"version": "0.2",
	"description": "Used for filling out website info",
	"background":{
		"persistent": true,
		"scripts": ["js/background.js"]
	},
	"browser_action":{
		"default_icon": "icons/vulpix.png"
	},
	"permissions" :[
		"tabs",
		"http://*/*",
		"https://*/*",
		"activeTab"
	],
	"icons": {
		"16": "icons/vulpix.png",
		"32": "icons/vulpix.png"
	}
}