{
	"manifest_version": 2,
	"name": "PrivateEyesAutoFill",
	"version": "0.5",
	"description": "Used for filling out website info",
	"background":{
		"persistent": true,
		"scripts": ["js/background.js"]
	},
	"browser_action":{
		"default_icon": "icons/vulpix.png"
	},
	"permissions" :[
		"tabs",
		"http://*/*",
		"https://*/*",
		"activeTab"
	],
	"commands":{
		"toggle-foo":{
			"suggested_key":{
			"default":"Ctrl+Shift+4",
			"mac": "Command + Shift+4"
			},
			
			"description":"script",
			"global": true
		}
	},
	"icons": {
		"16": "icons/vulpix.png",
		"32": "icons/vulpix.png"
	}
}
